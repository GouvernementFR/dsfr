<%
    const dependencies = styles.filter((p) => {
      if (required.indexOf(p) > -1) return true;
      if (locals.id && usage.styles[id] && usage.styles[id].indexOf(p) > -1) return true;
      if (locals.example && locals.example.styles) {
        if (example.styles.some((d) => { return usage.styles[d].indexOf(p) > -1; } )) return true;
      }
      return false;
    });
    for (let dependency of dependencies) {
    %>
<link href="<%= locals.isList ? '..' : '../..'%>/dist/css/<%= dependency %>.css" rel="stylesheet" >
<% } %>
