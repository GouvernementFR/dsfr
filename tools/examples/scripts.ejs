<%
    const dependencies = scripts.filter((p) => {
      if (required.indexOf(p) > -1) return true;
      if (locals.id && usage.scripts[id] && usage.scripts[id].indexOf(p) > -1) return true;
      if (locals.example && locals.example.scripts) {
        if (example.scripts.some((d) => { return usage.scripts[d].indexOf(p) > -1; } )) return true;
      }
      return false;
    });
    for (let dependency of dependencies) { %>
<script type="module" src="<%= locals.isList ? '..' : '../..'%>/dist/js/<%= dependency %>.module.js" ></script>
<script type="text/javascript" nomodule src="<%= locals.isList ? '..' : '../..'%>/dist/js/<%= dependency %>.nomodule.js" ></script>
<% } %>
