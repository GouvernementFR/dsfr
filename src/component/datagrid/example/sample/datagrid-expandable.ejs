<div class="<%= prefix %>-datagrid" role="table">
    <div class="<%= prefix %>-datagrid-thead" role="rowgroup">
        <div class="<%= prefix %>-datagrid-tr" role="row">
            <div
                class="<%= prefix %>-datagrid-th <%= prefix %>-datagrid-td--expandable"
                role="cell"
            ></div>
            <div class="<%= prefix %>-datagrid-th" role="columnheader">
                <span class="<%= prefix %>-datagrid-cell">Ticket ID</span>
            </div>
            <div class="<%= prefix %>-datagrid-th" role="columnheader">
                <span class="<%= prefix %>-datagrid-cell">Team</span>
            </div>
            <div class="<%= prefix %>-datagrid-th" role="columnheader">
                <span class="<%= prefix %>-datagrid-cell">Date</span>
            </div>
            <div class="<%= prefix %>-datagrid-th" role="columnheader">
                <span class="<%= prefix %>-datagrid-cell">Status</span>
            </div>
        </div>
    </div>
    <div class="<%= prefix %>-datagrid-tbody" role="rowgroup">
        <div class="<%= prefix %>-datagrid-tr" role="row" data-row="0">
            <div
                class="<%= prefix %>-datagrid-td <%= prefix %>-datagrid-td--expandable"
                role="cell"
            >
                <button
                    class="<%= prefix %>-btn <%= prefix %>-icon-arrow-down-s-line <%= prefix %>-btn--tertiary-no-outline <%= prefix %>-btn--sm"
                    title="Déplier"
                >
                    Déplier
