<%
    let input = locals.input || {}
    let jsonData = JSON.parse(include('../../../form/example/sample/data', {form:input}));
    let data = {...input, ...jsonData};
    data.type = input.type || 'text';
    data.label = 'Label champs de saisie';
%>
<% if (input.group === true) { %>
<%- include('../../template/ejs/input-group', {input:data}); %>
<% }
else { %>
<%- include('../../template/ejs/input', {input:data}); %>
<% } %>
