
<%
const sample = getSample(include);
%>

<%-
    sample(getText('sample.simple', 'table'), './sample/table-simple', {table: {table: {caption: getText('data.caption', 'table')}}}, true);
%>

<%-
    sample(getText('sample.sm', 'table'), './sample/table-simple', {table: {classes: [`${prefix}-table--sm`], table: {caption: getText('data.caption', 'table')}}}, true);
%>

<%-
    sample(getText('sample.lg', 'table'), './sample/table-simple', {table: {classes: [`${prefix}-table--lg`], table: {caption: getText('data.caption', 'table')}}}, true);
%>

<%-
    sample(getText('sample.complex', 'table'), './sample/table-complex', {table: {header: true, footer: true, table: {caption: getText('data.captionLarge', 'table'), rows: [{selected: true}, {expandable: 'true'}, {collapse: true}, {}, {expandable: 'false'}, {collapse: true}]}}}, true);
%>

<%-
    sample(getText('sample.selectable', 'table'), './sample/table-simple', {table: {table: {caption: getText('data.caption', 'table'), selectable: true}}}, true);
%>

<%-
    sample(getText('sample.expandable', 'table'), './sample/table-expandable', {}, true);
%>

