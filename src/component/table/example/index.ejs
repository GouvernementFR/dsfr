%>

<%-
    sample(getText('sample.cellCollapsed', 'table'), './sample/table-cell-collapsed', {}, true);
%>

<%-
    sample(getText('sample.doubleEntry', 'table'), './sample/table-simple', {table: {classes: [`${prefix}-table--lg`], table: {caption: getText('data.caption', 'table'), doubleEntry: true}}}, true);
