    sample(getText('sample.bordered', 'table'), './sample/table-simple', {table: {classes: [`${prefix}-table--bordered`], table: {caption: getText('data.caption', 'table'), doubleEntry: true, selectable: true, expandable: true, rows: [{expandable: 'false'}, {collapse: true}, {}]}}}, true);
%>

