<div class="<%= prefix %>-table">
    <div class="<%= prefix %>-table__wrapper">
        <div class="<%= prefix %>-table__container">
            <div class="<%= prefix %>-table__content">
                <table>
                    <caption data-fr-js-table-caption="true">Caption du tableau (accessibilit√©)</caption>
                    <thead>
                        <tr>
                            <th class="<%= prefix %>-cell__fixed <%= prefix %>-cell__fixed--last__right <%= prefix %>-cell__actionable" scope="col">
                                <div
                                    class="<%= prefix %>-checkbox-group <%= prefix %>-checkbox-group--sm"
                                >
                                    <input
                                        name="small-width-checkboxes-small--0"
                                        id="small-width-checkboxes-small--0"
                                        type="checkbox"
                                    />
                                    <label
                                        class="<%= prefix %>-label"
                                        for="small-width-checkboxes-small--0"
                                    >
                                    </label>
                                </div>
                            </th>
                            <th scope="col">Name</th>
                            <th scope="col">Age</th>
                            <th scope="col">Num</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr data-row-key="1">
                            <th class="<%= prefix %>-cell__fixed <%= prefix %>-cell__fixed--last__right <%= prefix %>-cell__actionable">
                                <div
                                    class="<%= prefix %>-checkbox-group <%= prefix %>-checkbox-group--sm"
                                >
                                    <input
                                        name="small-width-checkboxes-small--1"
                                        id="small-width-checkboxes-small--1"
                                        type="checkbox"
                                    />
                                    <label
                                        class="<%= prefix %>-label"
                                        for="small-width-checkboxes-small--1"
                                    >
                                    </label>
                                </div>
                            </th>
                            <td>
                                <a href="#">John Brown</a>
                            </td>
                            <td>32</td>
                            <td>0571-22098909</td>
                        </tr>
                        <tr data-row-key="3">
                            <th class="<%= prefix %>-cell__fixed <%= prefix %>-cell__fixed--last__right <%= prefix %>-cell__actionable">
                                <div
                                    class="<%= prefix %>-checkbox-group <%= prefix %>-checkbox-group--sm"
                                >
                                    <input
                                        name="small-width-checkboxes-small--3"
                                        id="small-width-checkboxes-small--3"
                                        type="checkbox"
                                    />
                                    <label
                                        class="<%= prefix %>-label"
                                        for="small-width-checkboxes-small--3"
                                    >
                                    </label>
                                </div>
                            </th>
                            <td>
                                <a href="#">Joe Black</a>
                            </td>
                            <td>32</td>
                            <td>0575-22098909</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
