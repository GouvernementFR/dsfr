<%- sample(getText('sample.double', 'range'), './sample/range-double.ejs', { range: {min: 0, max: 100, value: 25, value2: 75}}, true); %>

<%- sample(getText('sample.double-sm', 'range'), './sample/range-double.ejs', { range: {size: 'sm', min: 0, max: 100, value: 25, value2: 75}}, true); %>

