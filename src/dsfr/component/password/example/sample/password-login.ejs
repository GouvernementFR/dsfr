<%
let password = locals.password || {};

let data = {
  id: uniqueId('password'),
  ...password,
  input: {label: getText('password', 'password'), autocomplete: 'current-password', ...password.input},
  checkbox: {...password.checkbox},
  format: false,
  link: {
    href: contentPlaceholder(getText('link.href', 'password')),
    label: getText('link.label', 'password'),
    ...password.link,
  }
};
%>

<%- include('../../template/ejs/password.ejs', {password: data}); %>
