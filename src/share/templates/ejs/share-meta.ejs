<% eval(include('../../../core/index.ejs')); %>

<% for (const item of locals.items) { %>
    <% if(item.property) { %>
        <meta property="<%= item.property %>" content="<%= item.content %>">
    <% } else if(item.name) { %>
        <meta name="<%= item.name %>" content="<%= item.content %>">
    <% }
} %>
