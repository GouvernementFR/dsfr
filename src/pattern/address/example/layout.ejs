
<% if (locals.title !== undefined && locals.title.length) { %>
    <div class="<%= prefix %>-grid-row <%= prefix %>-mb-6v" >
        <div class="<%= prefix %>-col-12">
            <h4><%= title %></h4>
        </div>
    </div>
<% } %>
<div class="<%= prefix %>-grid-row <%= prefix %>-mb-6v" >
    <div class="<%= prefix %>-col-md-4 <%= prefix %>-col-12">
        <%- component %>
    </div>
</div>
<% if (locals.snippet !== undefined) { %>
    <div class="<%= prefix %>-grid-row <%= prefix %>-mb-6v" >
        <div class="<%= prefix %>-col-12">
            <div class="<%= prefix %>-mb-12v">
                <%- snippet %>
            </div>
        </div>
    </div>
<% } %>
