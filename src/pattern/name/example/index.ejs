<%
const sample = getSample(include);
%>

<%- section(getText('section.default', 'name'), '', 0); %>

<%- sample(getText('sample.default', 'name'), './sample/name', {}, true); %>

<%- sample(`${getText('sample.with', 'name')} ${getText('sample.firstname-disabled', 'name')}`, './sample/firstname-disabled', {}, true); %>

<%- sample(`${getText('sample.with', 'name')} ${getText('sample.usage-name', 'name')}`, './sample/usage-name', {}, true); %>

<%- sample(`${getText('sample.with', 'name')} ${getText('sample.usage-name', 'name')} + ${getText('sample.firstname-disabled', 'name')}`, './sample/usage-name-and-firstname-disabled', {}, true); %>

<%- sample(`${getText('sample.with', 'name')} ${getText('sample.button', 'name')}`, './sample/button', {}, true); %>

<%- sample(`${getText('sample.with', 'name')} ${getText('sample.button', 'name')} + ${getText('sample.firstname-disabled', 'name')}`, './sample/button-and-firstname-disabled', {}, true); %>

<%- section(getText('section.international', 'name'), '', 0); %>

<%- sample(getText('sample.default', 'name'), './sample/international', {}, true); %>

<%- sample(`${getText('sample.with', 'name')} ${getText('sample.firstname-disabled', 'name')}`, './sample/international-and-firstname-disabled', {}, true); %>
