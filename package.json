{
  "name": "@gouvfr/dsfr",
  "version": "2.0.0-lumières",
  "description": "Système de Design de l'Etat - DSFR",
  "repository": "git@github.com:GouvernementFR/dsfr.git",
  "author": "Service d'Information du Gouvernement <maxime.beaugrand@pm.gouv.fr>",
  "license": "SEE LICENSE IN LICENSE.md",
  "type": "module",
  "config": {
    "prefix": "fr",
    "namespace": "dsfr",
    "organisation": "@gouvfr"
  },
  "engines": {
    "node": ">=18.16.1"
  },
  "scripts": {
    "ng": "ng",
    "ng-start": "ng serve",
    "ng-build": "ng build",
    "ng-watch": "ng build --watch --configuration development",
    "ng-test": "ng test",
    "storybook": "storybook dev -p 6006 -c .storybook/html",
    "storybook-react": "storybook dev -p 6007 -c .storybook/react",
    "storybook-vue": "storybook dev -p 6008 -c .storybook/vue",
    "storybook-angular": "ng run dsfr:storybook",
    "build-storybook": "storybook build -c .storybook/html -o .doc/storybook/html",
    "build-storybook-react": "storybook build -c .storybook/react -o .doc/storybook/react",
    "build-storybook-vue": "storybook build -c .storybook/vue -o .doc/storybook/vue",
    "build-storybook-angular": "ng run dsfr:build-storybook"
  },
  "files": [
    "/dist",
    "/src",
    "/doc",
    "SECURITY.md",
    "CONTRIBUTING.md"
  ],
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular/animations": "^17.3.0",
    "@angular/cli": "^17.3.6",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/compiler-cli": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@chromatic-com/storybook": "^1.4.0",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@compodoc/compodoc": "^1.1.24",
    "@storybook/addon-docs": "^8.1.2",
    "@storybook/addon-essentials": "^8.1.2",
    "@storybook/addon-interactions": "^8.1.2",
    "@storybook/addon-links": "^8.1.2",
    "@storybook/angular": "^8.1.2",
    "@storybook/blocks": "^8.1.2",
    "@storybook/html": "^8.1.2",
    "@storybook/html-vite": "^8.1.2",
    "@storybook/react": "^8.1.2",
    "@storybook/react-vite": "^8.1.2",
    "@storybook/test": "^8.1.2",
    "@storybook/vue3": "^8.1.2",
    "@storybook/vue3-vite": "^8.1.2",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.12.12",
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.0",
    "express": "^4.18.2",
    "husky": "^7.0.4",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prop-types": "^15.8.1",
    "rxjs": "~7.8.0",
    "standard": "^16.0.4",
    "storybook": "^8.1.2",
    "stylelint": "^16.5.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-scss": "^6.3.0",
    "tslib": "^2.3.0",
    "typescript": "~5.4.2",
    "vite": "^5.2.8",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.19",
    "zone.js": "~0.14.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
