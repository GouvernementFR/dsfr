<% if (locals.prepend === true) { %>
  <%- include(`${root}/${path}/example/prepend.ejs`) %>
<% } %>

<% if (!locals.noHeading) { %>
  <%- include('heading'); %>
<% } %>
<%
switch(type) {
  case 'package':
%>
<%- include(root + 'tool/example/package', {type:undefined}); %>
<%
  break;

  case 'folder':
%>
<%- include(root + 'tool/example/folder', {type:undefined}); %>
<%
  break;
}
%>

<% if (locals.subdir.length) { %><%- include('subdir'); %><% } %>




