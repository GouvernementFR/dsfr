<%
    for (const module of files.script.module) {
      %>
<script type="module" src="<%= relativeRoot %><%= module %>" ></script>
    <%
    }

    for (const nomodule of files.script.nomodule) {
      %>
<script type="text/javascript" nomodule src="<%= relativeRoot %><%= nomodule %>" ></script>
    <%
    }
%>
