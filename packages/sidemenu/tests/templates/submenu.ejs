<%- include('./btn-sub'); %>
<div class="<%= prefix %>-sidemenu__content" id="<%= collapseId + '-sub' %>" >
  <ul class="<%= prefix %>-sidemenu__list" >
      <%
      for (let i = 0; i < subitems.length; i++) {
          let subitem = subitems[i];
          let classes = [prefix + '-sidemenu__item'];
      %>
      <li <%- includeClasses(classes); %>>
          <% if (!subitem.active) { %><a class="<%= prefix%>-link" href="#" target="_self" ><% } %>
              <%- subitem.label %>
          <% if (!subitem.active) { %></a><% } %>
      </li>
      <% } %>
  </ul>
</div>
